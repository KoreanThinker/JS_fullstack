{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "tsc -w",
    "migrate:save": "prisma migrate save --name init --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "studio:prisma": "prisma studio --experimental ",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --notify --respawn --transpile-only src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nexus/schema": "0.15.0",
    "apollo-server": "^2.17.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "nexus-plugin-prisma": "0.18.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.10.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
